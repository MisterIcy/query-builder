{
    "name": "mistericy/query-builder",
    "description": "MySQL Query Builder Library",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.98",
        "squizlabs/php_codesniffer": "^3.6",
        "phpmetrics/phpmetrics": "^2.7"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "MisterIcy\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueryBuilder\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alexandros Koutroulis",
            "email": "icyd3mon@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/polyfill-php80": "^1.0",
        "symfony/polyfill-php81": "^1.0"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit",
        "analyze": "./vendor/bin/phpstan"
    }
}
